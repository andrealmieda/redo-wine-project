{
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "import/no-extraneous-dependencies": ["error", {}],
        "operator-linebreak": ["error", "after"],
        "object-curly-newline": ["error", { "consistent": true }],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent": [1, 4]
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
    }
}